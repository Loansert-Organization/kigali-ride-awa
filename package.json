    "jsdom": "^26.1.0",
    "postcss": "^8.4.47",
    "prettier": "^3.1.1",
    "prettier-plugin-tailwindcss": "^0.5.9",
    "tailwindcss": "^3.4.11",
    "terser": "5.43.1",
    "ts-prune": "0.10.3",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.0.1",
