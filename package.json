{
  "name": "kigali-ride-awa",
  "displayName": "Kigali Ride AWA",
  "description": "A ride-sharing platform for Kigali with driver and passenger interfaces",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "npm run type-check && vite build",
    "build:with-lint": "npm run type-check && npm run lint:fix && vite build",
    "build:dev": "vite build --mode development",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "preview": "vite preview",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:watch": "vitest --watch",
    "analyze": "vite build --mode production && vite-bundle-visualizer",
    "clean": "rm -rf dist node_modules .turbo",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "rls:test": "ts-node scripts/rls-smoke.ts",
    "lint:a11y": "eslint src --ext .tsx,.ts --max-warnings=0"
  },
  "dependencies": {
    "@googlemaps/js-api-loader": "^1.16.8",
    "@googlemaps/markerclusterer": "^2.5.3",
